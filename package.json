{
  "name": "alphanumerics",
  "version": "0.9.6",
  "description": "Alphanumerics is a JavaScript library that encodes and decodes letters of english alphabet into numbers.",
  "homepage": "https://github.com/MFM-347/Alphanumerics#readme",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/an.js",
      "require": "./dist/an.umd.cjs"
    }
  },
  "main": "./dist/an.umd.cjs",
  "module": "./dist/an.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "format": "prettier -w .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "encode",
    "decode",
    "alphanumeric",
    "text encoding",
    "string manipulation",
    "utility",
    "ts",
    "mfm3478"
  ],
  "devDependencies": {
    "@types/node": "^18.14.0",
    "prettier": "^3.4.2",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.3",
    "vitest": "^2.1.8"
  },
  "bugs": {
    "url": "https://github.com/MFM-347/Alphanumerics/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MFM-347/Alphanumerics.git"
  },
  "author": "MFM-347",
  "license": "MIT"
}
