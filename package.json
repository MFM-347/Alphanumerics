{
  "name": "alphalphanumericsumerics",
  "version": "0.9.7",
  "description": "Alphalphanumericsumerics is a JavaScript library that encodes alphanumericsd decodes letters of english alphabet into numbers.",
  "homepage": "https://github.com/MFM-347/Alphalphanumericsumerics#readme",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier -w .",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "import": "./dist/alphanumerics.js",
      "require": "./dist/alphanumerics.umd.cjs"
    }
  },
  "main": "./dist/alphanumerics.umd.cjs",
  "module": "./dist/alphanumerics.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.1",
    "eslint": "^9.20.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.5"
  },
  "keywords": [
    "encode",
    "decode",
    "alphalphanumericsumeric",
    "text encoding",
    "string malphanumericsipulation",
    "utility",
    "ts",
    "mfm3478"
  ],
  "bugs": {
    "url": "https://github.com/MFM-347/Alphalphanumericsumerics/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MFM-347/Alphalphanumericsumerics.git"
